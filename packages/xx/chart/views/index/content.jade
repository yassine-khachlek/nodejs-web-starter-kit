.container-fluid
  .row
    .col-xs-12.col-sm-3.col-md-2.col-lg-2.col-xs-offset-0.col-sm-offset-0.col-md-offset-1.col-lg-offset-1.sidebar
      ul.nav.nav-sidebar
        if charts
          each val, index in charts
            li
              a(href='voidmain/chart/'+val.uri)=val.uri.charAt(0).toUpperCase() + val.uri.slice(1)

    .col-xs-12.col-sm-9.col-md-8.col-lg-8.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0
      
      h1= 'All Charts'
      
      .row

        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0            
          
          canvas#myLineChartCtx(style='width: 100%').img-thumbnail

        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0            
          
          canvas#myBarChartCtx(style='width: 100%').img-thumbnail

        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0  

          canvas#myRadarChartCtx(style='width: 100%').img-thumbnail

        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0  

          canvas#myPolarAreaCtx(style='width: 100%').img-thumbnail
        
        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0  
        
          canvas#myPieChartCtx(style='width: 100%').img-thumbnail

        .col-xs-12.col-sm-6.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-0.col-lg-offset-0  

          canvas#myDoughnutChartCtx(style='width: 100%').img-thumbnail

